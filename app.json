{
  "expo": {
    "name": "Streamer",
    "slug": "Streamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon-blue.png",
    "scheme": "Streamer",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/icon-blue.png",
      "resizeMode": "contain",
      "backgroundColor":"#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/icon-blue.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/icon-blue.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
